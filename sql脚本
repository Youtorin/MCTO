CREATE TABLE `Food` (
  `id` varchar(36) NOT NULL COMMENT '主键',
  `shopid` varchar(36) DEFAULT '0' COMMENT '商店ID',
  `cateid` varchar(36) DEFAULT '0' COMMENT '分类ID',
  `catename` varchar(255) DEFAULT NULL COMMENT '分类名称',
  `title` varchar(50) DEFAULT NULL COMMENT '食品名字',
  `description` varchar(100) DEFAULT NULL COMMENT '描述',
  `cover` varchar(3000) DEFAULT NULL COMMENT '食品封面图',
  `originPrice` decimal(10,0) DEFAULT '0' COMMENT '原价',
  `sellPrice` decimal(10,0) DEFAULT '0' COMMENT '售价',
  `number` int(11) DEFAULT NULL COMMENT '库存',
  `discount` decimal(10,0) DEFAULT '10' COMMENT '折扣',
  `likeNum` int(11) DEFAULT '0' COMMENT '点赞',
  `limitNum` int(11) DEFAULT '0' COMMENT '限购数量',
  `options` varchar(500) DEFAULT NULL COMMENT '规格选项',
  `totalSales` int(11) DEFAULT NULL COMMENT '总销量',
  `monthSales` int(11) DEFAULT NULL COMMENT '月销量',
  `praiseRate` float DEFAULT '100' COMMENT '好评率',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='菜品信息表';

CREATE TABLE `FoodCategory` (
  `id` varchar(36) NOT NULL COMMENT '主键',
  `shopId` varchar(36) DEFAULT '0' COMMENT '商店ID',
  `name` varchar(50) DEFAULT NULL COMMENT '分类类型',
  `description` varchar(500) DEFAULT NULL COMMENT '描述',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='食品分类表';

CREATE TABLE `Order` (
  `id` varchar(36) NOT NULL COMMENT '主键',
  `userId` varchar(36) DEFAULT '0' COMMENT '用户ID',
  `username` varchar(20) DEFAULT NULL COMMENT '用户名',
  `userMobile` varchar(20) DEFAULT NULL COMMENT '用户联系电话',
  `userAddressId` varchar(36) DEFAULT '0' COMMENT '用户地址ID',
  `userAddress` varchar(500) DEFAULT NULL COMMENT '用户详细地址',
  `distance` double DEFAULT NULL COMMENT '用户地址与商铺地址距离',
  `remark` varchar(255) DEFAULT NULL COMMENT '用户备注内容',
  `shopId` varchar(36) DEFAULT '0' COMMENT '商铺ID',
  `Shopname` varchar(20) DEFAULT NULL COMMENT '商铺名字',
  `shopMobile` varchar(20) DEFAULT NULL COMMENT '商铺联系电话',
  `shopAddress` varchar(500) DEFAULT NULL COMMENT '商铺详细地址',
  `food` text COMMENT '商品详情-json',
  `totalMoney` decimal(10,0) DEFAULT '0' COMMENT '总价',
  `distributionType` int(11) DEFAULT NULL COMMENT '配送方式 0-送货上门  1-自取',
  `sendCost` decimal(10,0) DEFAULT '0' COMMENT '配送费',
  `payMoney` decimal(10,0) DEFAULT '0' COMMENT '退回款项',
  `refundReason` varchar(500) DEFAULT NULL COMMENT '退款原因',
  `applyTime` datetime DEFAULT NULL COMMENT '申请时间',
  `handleTime` datetime DEFAULT NULL COMMENT '卖家处理时间',
  `finishTime` datetime DEFAULT NULL COMMENT '退款完成时间',
  `refundStatus` int(11) DEFAULT NULL COMMENT '退款状态 1-退款中 2-已完成 3-已取消',
  `demandTime` int(11) DEFAULT '0' COMMENT '限定时间',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（\r\n1-未支付待处理 \r\n2-已支付待出库\r\n3-已出库待收货\r\n4-已完成订单\r\n5-退款订单\r\n-1-已退款\r\n）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='订单主表';

CREATE TABLE `OrderComment` (
  `id` varchar(36) NOT NULL COMMENT '主键',
  `orderId` varchar(36) DEFAULT NULL COMMENT '订单ID',
  `userId` varchar(36) DEFAULT '0' COMMENT '用户ID',
  `username` varchar(255) DEFAULT NULL COMMENT '用户名',
  `userImg` varchar(255) DEFAULT NULL COMMENT '用户头像',
  `content` varchar(500) DEFAULT NULL COMMENT '评论内容',
  `images` varchar(1000) DEFAULT NULL COMMENT '评论图片',
  `numPraise` int(11) DEFAULT '0' COMMENT '点赞次数',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  `score` int(11) DEFAULT NULL COMMENT '评分',
  PRIMARY KEY (`id`),
  UNIQUE KEY `order_id` (`orderId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='订单--评论';

CREATE TABLE `OrderDetail` (
  `id` varchar(36) NOT NULL COMMENT '主键',
  `orderId` varchar(36) NOT NULL COMMENT '订单ID',
  `userUsername` varchar(20) DEFAULT NULL COMMENT '用户名',
  `userMobile` varchar(20) DEFAULT NULL COMMENT '用户联系电话',
  `remark` varchar(255) DEFAULT NULL COMMENT '用户备注内容',
  `userAddressId` varchar(36) DEFAULT '0' COMMENT '用户地址ID',
  `userLongitude` varchar(20) DEFAULT NULL COMMENT '经度',
  `userLatitude` varchar(20) DEFAULT NULL COMMENT '纬度',
  `userAddress` varchar(500) DEFAULT NULL COMMENT '用户详细地址',
  `shopShopname` varchar(20) DEFAULT NULL COMMENT '商铺名字',
  `shopMobile` varchar(20) DEFAULT NULL COMMENT '商铺联系电话',
  `shopAddress` varchar(500) DEFAULT NULL COMMENT '商铺详细地址',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  PRIMARY KEY (`id`),
  UNIQUE KEY `order_id` (`orderId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='订单详情表';

CREATE TABLE `OrderFood` (
  `id` varchar(36) NOT NULL COMMENT '主键',
  `orderId` varchar(36) DEFAULT NULL COMMENT '订单ID',
  `shopId` varchar(36) DEFAULT '0' COMMENT '商铺ID',
  `shopname` varchar(50) DEFAULT NULL COMMENT '商铺名称',
  `foodId` varchar(5000) DEFAULT '0' COMMENT '商品ID',
  `title` varchar(5000) DEFAULT NULL COMMENT '商品标题',
  `cover` varchar(5000) DEFAULT NULL COMMENT '商品封面',
  `sellPrice` decimal(10,0) DEFAULT '0' COMMENT '售价',
  `number` int(11) DEFAULT '0' COMMENT '下单数量',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='订单商品详情表';

CREATE TABLE `OrderRefund` (
  `id` varchar(36) NOT NULL COMMENT '主键',
  `orderId` varchar(36) DEFAULT NULL COMMENT '订单ID',
  `userId` varchar(36) DEFAULT '0' COMMENT '用户ID',
  `username` varchar(20) DEFAULT NULL COMMENT '用户名',
  `payMoney` decimal(10,0) DEFAULT '0' COMMENT '需要退回的款项',
  `refundReason` varchar(500) DEFAULT NULL COMMENT '退款原因',
  `applyTime` int(11) DEFAULT '0' COMMENT '申请时间',
  `handleTime` int(11) DEFAULT '0' COMMENT '卖家处理时间',
  `finishTime` int(11) DEFAULT '0' COMMENT '退款完成时间',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='订单--退款处理';

CREATE TABLE `Shop` (
  `id` varchar(36) NOT NULL COMMENT '编号主键',
  `shopname` varchar(50) DEFAULT NULL COMMENT '商店名称',
  `ShopAccount` varchar(255) DEFAULT NULL COMMENT '商店账号',
  `password` varchar(50) DEFAULT NULL COMMENT '商店密码',
  `todoItem` text COMMENT '待办事项',
  `wallet` decimal(10,0) DEFAULT NULL COMMENT '商店余额',
  `mobile` varchar(20) DEFAULT NULL COMMENT '手机号',
  `email` varchar(50) DEFAULT NULL COMMENT '邮箱',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  `remark` varchar(500) DEFAULT NULL COMMENT '简介',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='商家登录表';

CREATE TABLE `ShopInfo` (
  `id` varchar(36) NOT NULL COMMENT '编号主键',
  `shopid` varchar(36) DEFAULT NULL COMMENT '商店id',
  `shopname` varchar(500) DEFAULT NULL COMMENT '商店名称',
  `contactman` varchar(200) DEFAULT NULL COMMENT '联系人',
  `contactmobile` varchar(200) DEFAULT NULL COMMENT '联系电话',
  `cateid` int(11) DEFAULT '0' COMMENT '门店类型',
  `begintime` varchar(100) DEFAULT '0' COMMENT '营业开始时间',
  `endtime` varchar(100) DEFAULT '0' COMMENT '营业结束时间',
  `storeimg` longtext COMMENT '门店图片',
  `instoreimg` longtext COMMENT '店内图片',
  `logoimg` longtext COMMENT 'logo图片',
  `district` varchar(20) DEFAULT NULL COMMENT '省市区',
  `address` varchar(255) DEFAULT NULL COMMENT '详细地址',
  `longitude` varchar(20) DEFAULT NULL COMMENT '经度',
  `latitude` varchar(20) DEFAULT NULL COMMENT '纬度',
  `mobile` varchar(20) DEFAULT NULL COMMENT '外卖电话',
  `email` varchar(50) DEFAULT NULL COMMENT '邮箱',
  `notice` varchar(500) DEFAULT NULL COMMENT '商家公告',
  `score` float DEFAULT '0' COMMENT '平均评分',
  `sendtime` varchar(50) DEFAULT NULL COMMENT '配送时间',
  `sendcost` decimal(10,0) DEFAULT '0' COMMENT '配送费用',
  `floorSendCost` decimal(10,0) DEFAULT '0' COMMENT '起送消费',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='商铺信息表';

CREATE TABLE `User` (
  `id` varchar(36) NOT NULL COMMENT '编号主键',
  `username` varchar(50) DEFAULT NULL COMMENT '用户名',
  `password` varchar(50) DEFAULT NULL COMMENT '用户密码',
  `img` varchar(255) DEFAULT NULL COMMENT '用户头像',
  `mobile` varchar(20) DEFAULT NULL COMMENT '手机号',
  `wallet` decimal(10,0) DEFAULT '0' COMMENT '钱包',
  `email` varchar(50) DEFAULT NULL COMMENT '邮箱',
  `truename` varchar(50) DEFAULT NULL COMMENT '用户真实姓名',
  `gender` varchar(10) DEFAULT NULL COMMENT '性别',
  `viewNum` int(11) DEFAULT NULL COMMENT '访问次数',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='用户登录表';

CREATE TABLE `UserAddress` (
  `Id` varchar(36) NOT NULL COMMENT '编号主键',
  `UserId` varchar(36) DEFAULT NULL COMMENT '用户id',
  `username` varchar(50) DEFAULT NULL COMMENT '用户名',
  `gender` varchar(10) DEFAULT NULL COMMENT '性别',
  `mobile` varchar(20) DEFAULT NULL COMMENT '手机号',
  `district` varchar(50) DEFAULT NULL COMMENT '省市区',
  `longitude` varchar(200) DEFAULT NULL COMMENT '经度',
  `latitude` varchar(200) DEFAULT NULL COMMENT '维度',
  `address` varchar(200) DEFAULT NULL COMMENT '详细地址',
  `instance` float DEFAULT NULL COMMENT '距离',
  `street` varchar(200) DEFAULT NULL COMMENT '街道门牌号',
  `remark` varchar(200) DEFAULT NULL COMMENT '备注',
  `isDefault` int(11) DEFAULT '0' COMMENT '是否默认地址1-是 0-否',
  `CreateTime` datetime DEFAULT NULL COMMENT '创建时间',
  `ViewOrder` int(11) DEFAULT NULL COMMENT '显示序号',
  `Status` int(11) DEFAULT NULL COMMENT '状态（1-启用 0-禁用 -1-已删除）',
  `TimeStamp` timestamp NULL DEFAULT NULL COMMENT '记录更新时间戳',
  PRIMARY KEY (`Id`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='用户配送地址表';